                </engine:VisualElement>
                <engine:VisualElement name="do-damage"
                                      style="flex-grow: 1; background-color: rgb(0, 0, 0); flex-direction: row; height: 50px; width: 100%; font-size: 17px; color: rgb(221, 0, 0); -unity-font-style: bold; -unity-background-image-tint-color: rgb(255, 255, 255); -unity-text-outline-color: rgba(255, 0, 0, 0); visibility: visible; display: flex;">
                    <engine:Toggle label="Do damage" name="do-damage-toggle" style="color: rgb(128, 128, 128);"/>
                </engine:VisualElement>
                <engine:VisualElement name="damage-amount"
                                      style="flex-grow: 1; -unity-text-outline-color: rgb(243, 191, 191); background-color: rgb(0, 0, 0); flex-direction: row; height: 50px; width: 100%;">
