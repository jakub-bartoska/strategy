                <engine:Button text="CLEAR BATTLE" name="clear-battle-button"
                               style="align-self: center; display: flex; background-color: rgb(137, 137, 137); height: 70px; width: 200px; font-size: 21px; -unity-font-style: bold;"/>
            </engine:VisualElement>
            <engine:VisualElement name="debug-panel"
                                  style="flex-grow: 0; background-color: rgb(0, 0, 0); width: auto; align-items: flex-end; align-self: flex-end; flex-basis: 310px; flex-shrink: 0; -unity-text-outline-width: 0; -unity-text-outline-color: rgba(255, 255, 255, 0);">
                <engine:VisualElement name="battalion-speed"
